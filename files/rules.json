{
    "PreDNS": [
      {
        "ProxyRuleName": "domain-filter",
        "Description": "domain filter",
        "Action": "ALLOW",
        "Conditions": [
          {
            "ConditionOperator": "StringLike",
            "ConditionKey": "request:DestinationDomain",
            "ConditionValues": ["*.google.com", "*.bbc.co.uk"]
          }
        ]
      }
    ]
}